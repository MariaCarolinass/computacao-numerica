   "source": [
    "def f(x):\n",
    "    return np.exp(x / 2)\n",
    "\n",
    "def f3(x):\n",
    "    return (1 / 8) * np.exp(x / 2)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "88f149a4-1e16-49b8-b987-73bbd3b17fb0",
   "metadata": {},
   "outputs": [],
   "source": [
    "def r(x0, x):\n",
    "    return (f3(x) / 6) * (x - x0) ** 3"
   ]
  },
